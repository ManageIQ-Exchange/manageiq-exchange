{
  "title": "spin_doctor",
  "description": "This is the schema for the spin metadata",
  "type": "object",
  "properties": {
    "author": {
      "type": "string"
    },
    "company": {
      "type": [
        "string",
        "null"
      ]
    },
    "description": {
      "type": [
        "string",
        "null"
      ]
    },
    "metadata_version": {
      "type": "integer"
    },
    "min_miq_version": {
      "type": "string",
      "pattern": "^[a-z]$"
    },
    "max_miq_version": {
      "type": "string",
      "pattern": "^[a-z]$"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "author",
    "metadata_version",
    "min_miq_version",
    "tags"
  ]
}
